{
  "name": "airlink-panel",
  "version": "1.0.0",
  "description": "Airlink is a simple-to-use game server management panel",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/AirlinkLabs/panel"
  },
  "author": "AirlinkLabs",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run watch-ts\" \"npm run watch-css\" \"npm run start:dev\"",
    "build": "npm run build-ts && npm run build:css && npm run lint",
    "build-ts": "tsc",
    "build:css": "tailwindcss -i ./public/tw.conf -o ./public/styles.css",
    "start": "node --experimental-modules --es-module-specifier-resolution=node dist/app.js",
    "start:dev": "nodemon",
    "watch-ts": "tsc -w",
    "watch-css": "tailwindcss -i ./public/tw.conf -o ./public/styles.css --watch",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "migrate:dev": "prisma migrate dev --name init && prisma generate",
    "migrate:deploy": "prisma migrate deploy && prisma generate",
    "seed": "node --loader ts-node/esm src/handlers/cmd/seed.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "minecraft-status": "^1.1.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.13",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^6.0.0",
    "@types/express-session": "^1.18.1",
    "@types/express-ws": "^3.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "prisma": "^5.10.0",
    "supertest": "^6.3.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
